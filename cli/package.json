{
  "name": "@identity.com/cryptid-cli",
  "description": "Sign transactions and configure Cryptid DID Wallets",
  "version": "0.1.8",
  "author": "identity.com",
  "bin": {
    "cryptid": "./bin/run"
  },
  "bugs": "https://github.com/identity-com/cryptid/issues",
  "dependencies": {
    "@identity.com/cryptid": "^0.1.1",
    "@identity.com/sol-did-client": "^2.0.0",
    "@oclif/core": "^1.5.0",
    "@oclif/plugin-help": "^5.1.11",
    "@oclif/plugin-plugins": "^2.0.1",
    "@oclif/plugin-version": "^1",
    "@solana/spl-token": "^0.1.8",
    "@solana/web3.js": "^1.27.0",
    "did-resolver": "^3.1.0",
    "ramda": "^0.27.1",
    "tslib": "^1",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@oclif/test": "^2.1.0",
    "@types/chai": "^4.2.22",
    "@types/chai-as-promised": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.10.1",
    "@types/sinon": "^10.0.4",
    "@types/sinon-chai": "^3.2.5",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "globby": "^11",
    "mocha": "^9.1.2",
    "nyc": "^15.1.0",
    "oclif": "2.5.0",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "shx": "^0.3.4",
    "sinon": "^11.1.2",
    "sinon-chai": "^3.7.0",
    "source-map-support": "^0.5.20",
    "ts-node": "^8",
    "typescript": "^4.4.3"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/identity-com/cryptid",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "topicSeparator": " ",
    "commands": "./lib/commands",
    "bin": "cryptid",
    "plugins": [
      "@oclif/plugin-version",
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "additionalHelpFlags": [
      "-h"
    ],
    "additionalVersionFlags": [
      "-v"
    ]
  },
  "repository": "identity-com/cryptid",
  "scripts": {
    "build": "tsc -b",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "nyc mocha",
    "version": "oclif readme && git add README.md",
    "lint": "eslint . --ext .ts --max-warnings 0",
    "cryptid": "./bin/run"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "extension": ".ts",
    "cache": false,
    "exclude": [
      "lib/",
      "src/"
    ],
    "all": true,
    "skip-full": true,
    "check-coverage": true,
    "branches": 11,
    "lines": 58,
    "functions": 21,
    "statements": 57
  },
  "types": "lib/index.d.ts"
}
