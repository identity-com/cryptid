{
  "name": "@identity.com/cryptid",
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "nyc mocha test/unit",
    "lint": "eslint src/**/*.ts test/**/*.ts",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "test-e2e": "start-server-and-test start-validator http://localhost:8899/health test-e2e-pattern",
    "test-e2e-pattern": "mocha test/e2e",
    "start-validator": "solana-test-validator --bpf-program CFE6uDuLK7Hd9HVQqWkXU3nTmdZUm5KCEtFcEfbSTZLy ../target/deploy/cryptid_signer.so --bpf-program ide3Y2TubNMLLhiG1kDL6to4a8SjxD18YWCYC5BZqNV ../programs/cryptid_signer/tests/fixtures/sol_did_0.2.0.so --reset"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "skip-full": true,
    "exclude": [
      "dist/",
      "test/"
    ],
    "all": true,
    "check-coverage": true,
    "branches": 79,
    "lines": 90,
    "functions": 79,
    "statements": 92
  },
  "peerDependencies": {},
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "author": "identity.com",
  "module": "dist/client.esm.js",
  "size-limit": [
    {
      "path": "dist/client.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/client.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@size-limit/preset-small-lib": "^5.0.3",
    "@types/bs58": "^4.0.1",
    "@types/chai": "^4.2.21",
    "@types/chai-as-promised": "^7.1.4",
    "@types/chai-subset": "^1.3.3",
    "@types/chai-things": "^0.0.35",
    "@types/expect": "^24.3.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.9.6",
    "@types/sinon": "^10.0.2",
    "@types/sinon-chai": "^3.2.5",
    "bs58": "^4.0.1",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "chai-subset": "^1.6.0",
    "chai-things": "^0.2.0",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "mocha": "^9.1.1",
    "nyc": "^15.1.0",
    "sinon": "^11.1.2",
    "sinon-chai": "^3.7.0",
    "size-limit": "^5.0.3",
    "start-server-and-test": "^1.14.0",
    "ts-mocha": "^8.0.0",
    "ts-node": "^10.2.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@identity.com/sol-did-client": "^0.2.1",
    "@solana/web3.js": "^1.27.0",
    "@types/ramda": "^0.27.1",
    "did-resolver": "^3.1.0",
    "uint8arrays": "^3.0.0"
  }
